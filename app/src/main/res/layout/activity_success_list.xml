<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="ExtraText"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00C6FF"
    tools:context=".Success_List">

    <TextView
        android:id="@+id/list_your_c"
        android:layout_width="142dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="58dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="214dp"
        android:layout_marginBottom="843dp"
        android:gravity="top"
        android:layout_toEndOf="@id/arrow_14"
        android:text="@string/congratulations"
        android:textAppearance="@style/congratulation" />

    <ImageView
        android:id="@+id/arrow_14"
        android:layout_width="23dp"
        android:layout_height="23dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="31dp"
        android:background="@drawable/arrow_14"/>

    <View
        android:id="@+id/line_11"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="69dp"
        android:background="#FFFFFF"/>

    <ImageView
        android:id="@+id/check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/checked1"/>

    <TextView
        android:id="@+id/car_list"
        android:layout_width="209dp"
        android:layout_height="46dp"
        android:text="your_car_brezza_is_listed_successfully"
        android:layout_below="@id/check"
        android:textColor="@color/white"
        android:textAlignment="center"
        android:layout_marginStart="102dp"
        android:layout_marginTop="22dp"/>

    <Button
        android:layout_width="134dp"
        android:layout_height="49dp"
        android:layout_below="@+id/car_list"
        android:layout_centerInParent="true"
        android:text="go_back"
        android:background="@drawable/button_rect"
        android:textColor="#00C6FF"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/muli_extrabold"/>
</RelativeLayout>